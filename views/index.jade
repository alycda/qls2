extends layout

block content
    div(class="wrapper")
        div(class='left-dock')
            img(src='/images/queen-latifah.png', width=150, height=138)
            ul(class='nav')
                li(class='living')
                    a living
                li(class='loving')
                    a loving
                li(class='learning')
                    a learning
                li(class='laughing')
                    a laughing
                li(class='latifah')
                    a latifah
            div(class='weather')
                img(src='/images/weather.png')
        div(class='content')
            div(class='filter')
                img(src='/images/filter.png')

            div(id='tiles', class='container')

                div(class='pure-g-r')
                    div(class='pure-u-1-8 tile-1 wide tall') test
                    div(class='pure-u-1-8 tile-2 wide') test
                    div(class='pure-u-1-8 tile-3 wide tall') test
                    div(class='pure-u-1-8 tile-4') test
                    div(class='pure-u-1-8 tile-5 tall') test
                    div(class='pure-u-1-8 tile-6 ') test
                    div(class='pure-u-1-8 tile-7 wide tall') test
                    div(class='pure-u-1-8 tile-8') test
                    div(class='pure-u-1-8 tile-9 tall') test
                    div(class='pure-u-1-8 tile-10') test
                    div(class='pure-u-1-8 tile-11') test
                    div(class='pure-u-1-8 tile-1 wide tall') test
                    div(class='pure-u-1-8 tile-2 wide') test
                    div(class='pure-u-1-8 tile-3 wide tall') test
                    div(class='pure-u-1-8 tile-4') test
                    div(class='pure-u-1-8 tile-5 tall') test
                    div(class='pure-u-1-8 tile-6 ') test
                    div(class='pure-u-1-8 tile-7 wide tall') test
                    div(class='pure-u-1-8 tile-8') test
                    div(class='pure-u-1-8 tile-9 tall') test
                    div(class='pure-u-1-8 tile-10') test
                    div(class='pure-u-1-8 tile-11') test
                    div(class='pure-u-1-8 tile')

    div(class='right-dock')
            div(class='search')
                img(src='/images/search.jpg')
            div(class='social')
                img(src='/images/social.jpg')
            div(class='daily')
                img(src='/images/doobie.jpg')
            div(class='cta')
                img(src='/images/sidebar-photo.jpg')
                div(class='text')
                    h2
                        | Latest News
                    | The Queen Latifah Show Rocks Daytime Television

            div(class='twitter')
                img(src='/images/twitter.jpg')

