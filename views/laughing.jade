extends layout

block content
    div.myContainer
        div.row
            div.ads.
                <!-- begin ad tag (tile=1) -->
                <script type="text/javascript">
                //<![CDATA[
                ord=Math.random()*10000000000000000;
                document.write('<script type="text/javascript" src="http://ad.doubleclick.net/adj/queen.opus/laughing;tile=1;sz=728x90,970x90;ord=' + ord + '?"><\/script>');
                //]]>
                </script>
